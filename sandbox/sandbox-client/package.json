{
  "name": "my-cool-ext-web-components-app",
  "version": "0.0.1",
  "description": "my-cool-ext-web-components-app description for Ext-Web-Components app MyCoolExtWebComponentsApp",
  "keywords": [
    "ExtWebComponents"
  ],
  "license": "ISC",
  "scripts": {
    "build": "npm run clean && npm run webpackprod && npm run gzip",
    "clean": "rimraf build",
    "dev": "webpack-dev-server --env.browser=yes --env.verbose=no",
    "gzip": "npm run gzipbuild && npm run gzipext && npm run gzipresources",
    "gzipbuild": "npx gzip build/*.js",
    "gzipext": "npx gzip build/ext/*.js",
    "gzipresources": "npx gzip build/resources/*.js",
    "http": "npx http-server build -g -o",
    "replace": "npx replace-in-file /.js/g .js.gz build/index.html --isRegex",
    "start": "npm run dev",
    "webpackprod": "cross-env webpack --env.environment=production --env.treeshake=yes --env.basehref=/",
    "webpackdev": "cross-env webpack --env.environment=development --env.basehref=/ --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@sencha/ext-web-components": "~7.0.0",
